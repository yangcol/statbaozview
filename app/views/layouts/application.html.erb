<!DOCTYPE html>
<html>
<head>
  <title>Stat</title>
  <%= stylesheet_link_tag 'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= stylesheet_link_tag 'bootstrap.min' %>
  <%= javascript_include_tag 'jquery.js' %>
  <%= javascript_include_tag 'bootstrap.min' %>
  <%= csrf_meta_tags %>
</head>
<body>
<nav class="navbar navbar-inverse navbar-fixed-top">
  <div class="container-fluid">
    <div class="navbar-header">
      <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#navbar" aria-expanded="false" aria-controls="navbar">
        <span class="sr-only">Toggle navigation</span>
        <span class="icon-bar"></span>
        <span class="icon-bar"></span>
        <span class="icon-bar"></span>
      </button>
      <a class="navbar-brand" href="#">统计系统</a>
    </div>

    <div class="nav nav-pills pull-right">
      <%= render 'layouts/navigation_links' %>
    </div>
    <div id="navbar" class="collapse navbar-collapse">
      <ul class="nav navbar-nav">
        <!--class="active"-->
        <li><%= link_to 'ServerPV', serverpvs_path %></li>
        <li><%= link_to 'ServerMean', servermeans_path %></li>
        <li><%= link_to 'Slowlog', slowlogs_path %></li>
        <li><%= link_to 'SlowlogExts', slowlogexts_path %></li>
      </ul>
    </div>
    <!--/.nav-collapse -->
  </div>
</nav>


<div class="container">
  <div class="jumbotron">
    <div class="col-sm-10 col-sm-offset-0 col-md-10 col-md-offset-0 main">
      <div class="table-responsive">
        <%= yield %>
      </div>
    </div>
  </div>
</div>

<div class="container">
  <footer class="footer">
    <!--<p class="pull-right"><a href="#">返回顶部</a></p>-->
    <p>版权声明: @yangqing qing.yang@baoz.cn</p>
    <!--<p>版权声明： <a href="http://creativecommons.org/licenses/by/3.0/">CC BY 3.0</a>.</p>-->
  </footer>
</div>

</body>
</html>
