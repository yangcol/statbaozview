<p id="notice"><%= notice %></p>

<h1>Listing Slowlogs</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Key</th>
      <th>Duration</th>
      <th>Date</th>
      <th>Value</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @slowlogs.each do |slowlog| %>
      <tr>
        <td><%= slowlog.name %></td>
        <td><%= slowlog.key %></td>
        <td><%= slowlog.duration %></td>
        <td><%= slowlog.date %></td>
        <td><%= slowlog.value %></td>
        <td><%= link_to 'Show', slowlog %></td>
        <td><%= link_to 'Edit', edit_slowlog_path(slowlog) %></td>
        <td><%= link_to 'Destroy', slowlog, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>
<%= will_paginate @slowlogs, renderer: BootstrapPagination::Rails %>
<br>

<%= link_to 'New Slowlog', new_slowlog_path %>
